<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--定义InventoryInput表单视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_input_form_view">
            <field name="name">wjzpw.inventory.ruKuGuanLi</field>
            <field name="type">form</field>
            <field name="model">wjzpw.inventory.input</field>
            <field name="arch" type="xml">
                <form string="inventoryInput" version="7.0">
                    <sheet>
                        <group col="4">
                            <field name="machine_no"/>
                            <field name="input_date"/>
                            <field name="product_id"/>
                            <field name="batch_no"/>
                            <field name="superior_number"/>
                            <field name="grade_a_number"/>
                            <field name="grade_b_number"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!--定义InventoryInput列表视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_input_tree_view">
            <field name="name">wjzpw.inventory.ruKuGuanLi</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.inventory.input</field>
            <field name="arch" type="xml">
                <tree string="inventoryInput">
                    <field name="machine_no"/>
                    <field name="input_date"/>
                    <field name="product_id"/>
                    <field name="batch_no"/>
                    <field name="superior_number"/>
                    <field name="grade_a_number"/>
                    <field name="grade_b_number"/>
                </tree>
            </field>
        </record>
        <!--定义InventoryInput视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_inventory_input">
            <field name="name">wjzpw.inventory.ruKuGuanLi</field>
            <field name="res_model">wjzpw.inventory.input</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="wjzpw_inventory_input_tree_view"/>
        </record>

        <!--定义InventoryOutput表单视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_output_form_view">
            <field name="name">wjzpw.inventory.chuKuGuanLi</field>
            <field name="type">form</field>
            <field name="model">wjzpw.inventory.output</field>
            <field name="arch" type="xml">
                <form string="inventoryOutput" version="7.0">
                    <sheet>
                        <group col="4">
                            <field name="input_date"/>
                            <field name="code"/>
                            <field name="customer"/>
                            <field name="product_id"/>
                            <field name="batch_no"/>
                            <field name="superior_number"/>
                            <field name="grade_a_number"/>
                            <field name="grade_b_number"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!--定义InventoryOutput列表视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_output_tree_view">
            <field name="name">wjzpw.inventory.chuKuGuanLi</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.inventory.output</field>
            <field name="arch" type="xml">
                <tree string="inventoryOutput">
                    <field name="input_date"/>
                    <field name="code"/>
                    <field name="customer"/>
                    <field name="product_id"/>
                    <field name="batch_no"/>
                    <field name="superior_number"/>
                    <field name="grade_a_number"/>
                    <field name="grade_b_number"/>
                </tree>
            </field>
        </record>
        <!--定义InventoryOutput视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_inventory_output">
            <field name="name">wjzpw.inventory.chuKuGuanLi</field>
            <field name="res_model">wjzpw.inventory.output</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="wjzpw_inventory_output_tree_view"/>
        </record>

        <!--定义OrganzineInput表单视图-->
        <record model="ir.ui.view" id="wjzpw_organzine_input_form_view">
            <field name="name">wjzpw.inventory.ruKuGuanLi</field>
            <field name="type">form</field>
            <field name="model">wjzpw.organzine.input</field>
            <field name="arch" type="xml">
                <form string="organzineInput" version="7.0">
                    <sheet>
                        <group col="4">
                            <field name="input_date"/>
                            <field name="process_unit"/>
                            <field name="material_specification"/>
                            <field name="material_area"/>
                            <field name="batch_no"/>
                        </group>
                        <group col="2">
                            <field name="is_second"/>
                        </group>
                        <group col="4">
                            <field name="quantity"/>
                            <field name="count" attrs="{'invisible':[('is_second','=',False)]}"/>
                            <field name="height"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!--定义OrganzineInput列表视图-->
        <record model="ir.ui.view" id="wjzpw_organzine_input_tree_view">
            <field name="name">wjzpw.inventory.ruKuGuanLi</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.organzine.input</field>
            <field name="arch" type="xml">
                <tree string="organzineInput">
                    <field name="input_date"/>
                    <field name="process_unit"/>
                    <field name="material_specification"/>
                    <field name="material_area"/>
                    <field name="batch_no"/>
                    <field name="is_second"/>
                    <field name="quantity"/>
                    <field name="count"/>
                    <field name="height"/>
                </tree>
            </field>
        </record>
        <!--定义OrganzineInput视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_organzine_input">
            <field name="name">wjzpw.inventory.ruKuGuanLi</field>
            <field name="res_model">wjzpw.organzine.input</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="wjzpw_organzine_input_tree_view"/>
        </record>


        <!--定义OrganzineOutput表单视图-->
        <record model="ir.ui.view" id="wjzpw_organzine_output_form_view">
            <field name="name">wjzpw.inventory.chuKuGuanLi</field>
            <field name="type">form</field>
            <field name="model">wjzpw.organzine.output</field>
            <field name="arch" type="xml">
                <form string="organzineOutput" version="7.0">
                    <sheet>
                        <group col="4">
                            <field name="output_date"/>
                            <field name="process_unit" on_change="onchange_process_unit(process_unit)"/>
                            <field name="material_specification" on_change="onchange_material_specification(process_unit, material_specification)"/>
                            <field name="material_area" on_change="onchange_material_area(process_unit, material_specification, material_area)"/>
                            <field name="batch_no"/>
                        </group>
                        <group col="4">
                            <field name="quantity"/>
                            <field name="count"/>
                            <field name="height"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!--定义OrganzineOutput列表视图-->
        <record model="ir.ui.view" id="wjzpw_organzine_output_tree_view">
            <field name="name">wjzpw.inventory.chuKuGuanLi</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.organzine.output</field>
            <field name="arch" type="xml">
                <tree string="organzineOutput">
                    <field name="output_date"/>
                    <field name="process_unit"/>
                    <field name="material_specification"/>
                    <field name="material_area"/>
                    <field name="batch_no"/>
                    <field name="quantity"/>
                    <field name="count"/>
                    <field name="height"/>
                </tree>
            </field>
        </record>
        <!--定义OrganzineOutput视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_organzine_output">
            <field name="name">wjzpw.inventory.chuKuGuanLi</field>
            <field name="res_model">wjzpw.organzine.output</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="wjzpw_organzine_output_tree_view"/>
        </record>

        <!--定义InventoryMachineOutput表单视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_machine_output_form_view">
            <field name="name">wjzpw.inventory.jiTaiChanLiang</field>
            <field name="type">form</field>
            <field name="model">wjzpw.inventory.machine.output</field>
            <field name="arch" type="xml">
                <form string="inventoryMachineOutput" version="7.0">
                    <sheet>
                        <group col="4">
                            <field name="machine_no"/>
                            <field name="product_id"/>
                            <field name="batch_no"/>
                            <field name="beam_amount"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!--定义InventoryMachineOutput列表视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_machine_output_tree_view">
            <field name="name">wjzpw.inventory.jiTaiChanLiang</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.inventory.machine.output</field>
            <field name="arch" type="xml">
                <tree string="inventoryMachineOutput">
                    <field name="machine_no"/>
                    <field name="product_id"/>
                    <field name="batch_no"/>
                    <field name="beam_amount"/>
                    <field name="output_amount_fun"/>
                    <field name="complete_date_fun"/>
                    <field name="woven_shrinkage_fun"/>
                    <button string="wjzpw.inventory.wanChengBingJiSuanZhiSuoLv"
                            type="object"
                            name="action_button_complete_machine_output"
                            icon="gtk-index"
                            attrs="{'invisible':[ ('woven_shrinkage_fun','!=','未知')]}"/>
                </tree>
            </field>
        </record>
        <!--定义InventoryMachineOutput视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_inventory_machine_output">
            <field name="name">wjzpw.inventory.jiTaiChanLiang</field>
            <field name="res_model">wjzpw.inventory.machine.output</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="wjzpw_inventory_machine_output_tree_view"/>
        </record>

        <!--定义Inventory列表视图-->
        <record model="ir.ui.view" id="wjzpw_inventory_tree_view">
            <field name="name">wjzpw.inventory.kuCun</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.inventory</field>
            <field name="arch" type="xml">
                <tree string="inventoryInput" create="false" edit="false">
                    <field name="product_id"/>
                    <field name="batch_no"/>
                    <field name="superior_amount"/>
                    <field name="a_amount"/>
                    <field name="b_amount"/>
                </tree>
            </field>
        </record>
        <!--定义Inventory视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_cloth_inventory">
            <field name="name">wjzpw.inventory.kuCun</field>
            <field name="res_model">wjzpw.inventory</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="wjzpw_inventory_tree_view"/>
        </record>

        <!--定义OrganzineInventory列表视图-->
        <record model="ir.ui.view" id="wjzpw_organzine_inventory_tree_view">
            <field name="name">wjzpw.inventory.kuCun</field>
            <field name="type">tree</field>
            <field name="model">wjzpw.organzine.inventory</field>
            <field name="arch" type="xml">
                <tree string="organzineInventory" create="false" edit="false" delete="false">
                    <field name="process_unit"/>
                    <field name="material_specification"/>
                    <field name="material_area"/>
                    <field name="batch_no"/>
                    <field name="quantity"/>
                    <field name="count"/>
                    <field name="height"/>
                </tree>
            </field>
        </record>
        <!--定义OrganzineInventory视图动作-->
        <record model="ir.actions.act_window" id="action_wjzpw_organzine_inventory">
            <field name="name">wjzpw.inventory.kuCun</field>
            <field name="res_model">wjzpw.organzine.inventory</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="wjzpw_organzine_inventory_tree_view"/>
        </record>

        <!--定义菜单-->
        <menuitem id="wjzpw_inventory_menu" name="wjzpw.inventory.kuCun"/>
        <!-- 坯布库存 -->
        <menuitem id="wjzpw_cloth_menu" name="wjzpw.inventory.piBuKuCun" parent="wjzpw_inventory_menu"/>
        <menuitem id="wjzpw_cloth_inventory_menu" name="wjzpw.inventory.kuCun" parent="wjzpw_cloth_menu" action="action_wjzpw_cloth_inventory"/>
        <menuitem id="wjzpw_machine_output_menu" name="wjzpw.inventory.jiTaiChanLiang" parent="wjzpw_cloth_menu" action="inventory.action_wjzpw_inventory_machine_output"/>
        <menuitem id="wjzpw_cloth_input_menu" name="wjzpw.inventory.ruKu" parent="wjzpw_cloth_menu" action="inventory.action_wjzpw_inventory_input"/>
        <menuitem id="wjzpw_cloth_output_menu" name="wjzpw.inventory.chuKu" parent="wjzpw_cloth_menu" action="inventory.action_wjzpw_inventory_output"/>
        <!-- 原料库存 -->
        <menuitem id="wjzpw_organzine_menu" name="wjzpw.inventory.jingSiKuCun" parent="wjzpw_inventory_menu"/>
        <menuitem id="wjzpw_organzine_inventory_menu" name="wjzpw.inventory.kuCun" parent="wjzpw_organzine_menu" action="action_wjzpw_organzine_inventory"/>
        <menuitem id="wjzpw_organzine_input_menu" name="wjzpw.inventory.ruKu" parent="wjzpw_organzine_menu" action="action_wjzpw_organzine_input"/>
        <menuitem id="wjzpw_organzine_output_menu" name="wjzpw.inventory.chuKu" parent="wjzpw_organzine_menu" action="action_wjzpw_organzine_output"/>

        <menuitem id="wjzpw_weft_menu" name="wjzpw.inventory.weiSiKuCun" parent="wjzpw_inventory_menu"/>
        <menuitem id="wjzpw_weft_inventory_menu" name="wjzpw.inventory.cheJianShengYu" parent="wjzpw_weft_menu" action="wjzpw.action_wjzpw_product"/>
        <menuitem id="wjzpw_weft_input_menu" name="wjzpw.inventory.ruKu" parent="wjzpw_weft_menu" action="wjzpw.action_wjzpw_product"/>
        <menuitem id="wjzpw_tweft_output_menu" name="wjzpw.inventory.chuKu" parent="wjzpw_weft_menu" action="wjzpw.action_wjzpw_product"/>
        <!-- 经轴库存 -->
        <menuitem id="wjzpw_beam_menu" name="wjzpw.inventory.jingZhouKuCun" parent="wjzpw_inventory_menu"/>
        <menuitem id="wjzpw_beam_inventory_menu" name="wjzpw.inventory.kuCun" parent="wjzpw_beam_menu" action="wjzpw.action_wjzpw_product"/>
        <menuitem id="wjzpw_czpk_menu" name="wjzpw.inventory.chuanZongPaKou" parent="wjzpw_beam_menu" action="wjzpw.action_wjzpw_product"/>
        <!-- 设置 -->
        <menuitem id="wjzpw_setting_menu" name="wjzpw.inventory.sheZhi" parent="wjzpw_inventory_menu"/>
        <menuitem id="wjzpw_product_menu" name="wjzpw.inventory.pinMing" parent="wjzpw_setting_menu" action="wjzpw.action_wjzpw_product"/>
        <menuitem id="wjzpw_batch_no_menu" name="wjzpw.inventory.piHao" parent="wjzpw_setting_menu" action="wjzpw.action_wjzpw_batch_no"/>
        <menuitem id="wjzpw_organzine_batch_no_menu" name="wjzpw.inventory.jingSiPiHao" parent="wjzpw_setting_menu" action="wjzpw.action_wjzpw_organzine_batch_no"/>
        <menuitem id="wjzpw_weft_batch_no_menu" name="wjzpw.inventory.weiSiPiHao" parent="wjzpw_setting_menu" action="wjzpw.action_wjzpw_weft_batch_no"/>
        <menuitem id="wjzpw_material_specification_menu" name="wjzpw.inventory.yuanLiaoGuiGe" parent="wjzpw_setting_menu" action="wjzpw.action_wjzpw_material_specification"/>
    </data>
</openerp>